{
  "name": "@pocket/benchmarks",
  "version": "0.0.0",
  "private": true,
  "description": "Benchmark suite for Pocket database",
  "type": "module",
  "scripts": {
    "bench": "vitest bench --run --config vitest.config.ts",
    "bench:crud": "vitest bench --run --config vitest.config.ts src/suites/crud.bench.ts",
    "bench:query": "vitest bench --run --config vitest.config.ts src/suites/query.bench.ts",
    "bench:legacy": "tsx src/run.ts",
    "bench:legacy:core": "tsx src/core.bench.ts",
    "bench:legacy:query": "tsx src/query.bench.ts"
  },
  "dependencies": {
    "@pocket/core": "workspace:*",
    "@pocket/storage-memory": "workspace:*"
  },
  "devDependencies": {
    "tinybench": "^2.6.0",
    "tsx": "^4.7.0",
    "vitest": "^3.0.0"
  }
}
