{
  "name": "@pocket/ai",
  "version": "0.1.0",
  "description": "Offline AI assistant for Pocket with local LLM inference and RAG",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@pocket/core": "workspace:*",
    "@pocket/vectors": "workspace:*",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@types/node": "^25.0.10",
    "openai": "^4.20.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.20.0",
    "openai": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "keywords": [
    "pocket",
    "ai",
    "llm",
    "rag",
    "local-first",
    "offline",
    "vector-search",
    "embeddings"
  ]
}
